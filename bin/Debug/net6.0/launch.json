{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET Launch (web) - RailwayReservation",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build",
        "program": "${workspaceFolder}/src/RailwayReservation.Web/bin/Debug/net6.0/RailwayReservation.Web.dll",
        "args": [],
        "cwd": "${workspaceFolder}/src/RailwayReservation.Web",
        "env": {
          "ASPNETCORE_URLS": "http://localhost:5001"
        },
        "stopAtEntry": false,
        "console": "internalConsole",
        "internalConsoleOptions": "openOnSessionStart"
      }
    ]
  }
  